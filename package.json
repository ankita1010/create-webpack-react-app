{
	"name": "create-webpack-react-app",
	"version": "1.0.6",
	"description": "A command line to to initiate a boilerplate for a react application using webpack and babel.\nTo run the CLI, install the CLI globally by running \n$ npm i -g create-webpack-react-app \nand go to the directory where you want the root directory of your project to be and then run create-webpack-react-app project-name.\n",
	"main": "cli.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"cleantest": "rm -rf demo && create-webpack-react-app demo",
		"build": "rm -rf lib && babel ./src --out-dir lib --ignore ./src/template --copy-files  --include-dotfiles"
	},
	"bin": {
		"create-webpack-react-app": "./lib/cli.js",
		"dev-test": "./src/cli.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ankita1010/create-webpack-react-app.git"
	},
	"keywords": [
		"webpack",
		"react",
		"babel",
		"reactjs",
		"boilerplate"
	],
	"author": "Himayan Debnath, Ankita Chakraborty",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/ankita1010/create-webpack-react-app/issues"
	},
	"homepage": "https://github.com/ankita1010/create-webpack-react-app#readme",
	"dependencies": {
		"chalk": "^2.4.2",
		"clui": "^0.3.6",
		"inquirer": "^7.0.0",
		"minimist": "^1.2.0",
		"@babel/cli": "^7.7.0",
		"@babel/core": "^7.7.2",
		"@babel/polyfill": "^7.7.0",
		"@babel/preset-env": "^7.7.1"
	}
}
